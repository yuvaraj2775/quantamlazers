{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "typeRoots": [
      "types"
    ],
    "paths": {
      "@/components/": [
        "Components/"
      ],
      "@/actions/": [
        "actions/"
      ],
      "@/pages/": [
        "pages/"
      ],
      "@/app/": [
        "app/"
      ],
      "@/lib/": [
        "lib/"
      ],
      "@/libs/": [
        "libs/"
      ],
      "@/styles/": [
        "styles/"
      ]
    },
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "/.ts",
    "/.tsx",
    ".next/types//*.ts",
    "Components/DatePicker.js",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
